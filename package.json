    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.16.3",
    "babel-rewire-wrapper": "^1.0.2",
